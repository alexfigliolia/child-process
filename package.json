{
  "name": "@figliolia/child-process",
  "version": "1.0.0",
  "description": "A wrapper around the node.js spawn function providing a promise and graceful exiting",
  "keywords": [
    "node",
    "node.js",
    "child process"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "bin": "./dist/cjs/stealth/CLI.js",
  "files": [
    "dist",
    "src/*"
  ],
  "scripts": {
    "build": "ts-node ci/commands/Build.ts",
    "lint": "ts-node ci/commands/Lint.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@swc/core": "^1.3.99",
    "@types/jest": "^29.5.10",
    "@types/node": "^16.7.13",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^4.4.2"
  },
  "packageManager": "yarn@4.0.2"
}
